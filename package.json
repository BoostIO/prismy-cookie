{
  "name": "prismy-cookie",
  "version": "1.1.0",
  "description": ":cookie: Cookie for prismy",
  "keywords": [
    "prismy",
    "cookie"
  ],
  "author": "Junyoung Choi <rokt33r.choi@gmail.com>",
  "homepage": "https://github.com/prismyland/prismy-cookie",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prismyland/prismy-cookie.git"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "prettier --check src/**/*.ts specs/*/src/**/*.ts",
    "format": "prettier --write src/**/*.ts specs/*/src/**/*.ts",
    "test": "npm run lint && npm run test-type && npm run test-coverage",
    "test-api": "ava --verbose",
    "test-type": "tsc --noEmit",
    "test-coverage": "nyc ava && nyc report --reporter=text-lcov > coverage.lcov",
    "codecov": "codecov",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "bugs": {
    "url": "https://github.com/prismyland/prismy-cookie/issues"
  },
  "devDependencies": {
    "@types/cookie": "^0.3.3",
    "@types/got": "^9.4.4",
    "@types/node": "^13.7.2",
    "@types/test-listen": "^1.1.0",
    "@types/tough-cookie": "^2.3.5",
    "ava": "^2.2.0",
    "codecov": "^3.5.0",
    "got": "^9.6.0",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "prismy": "^1.2.0",
    "prismy-test": "^1.0.0",
    "rimraf": "^3.0.0",
    "tough-cookie": "^3.0.1",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.5"
  },
  "peerDependencies": {
    "prismy": ">= 1.2 < 2"
  },
  "dependencies": {
    "cookie": "^0.4.0"
  },
  "ava": {
    "compileEnhancements": false,
    "files": [
      "specs/*"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  }
}
